{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-underscore-dangle": [
        "error", 
        { 
          "allow": ["_tooltips"] 
        }
      ],
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "import/no-extraneous-dependencies": [
        "error", 
        {
          "devDependencies": true
        }
      ]
    }
  }